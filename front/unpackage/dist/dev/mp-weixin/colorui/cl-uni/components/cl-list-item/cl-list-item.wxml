<view data-event-opts="{{[['touchstart',[['onTouchStart',['$event']]]],['touchmove',[['onTouchMove',['$event']]]],['touchend',[['onTouchEnd',['$event']]]],['tap',[['onTap',['$event']]]]]}}" class="{{['cl-list-item',isAppend,isDisabled,isBorder]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e" catchtap="__e"><view class="cl-list-item__swiper" style="{{'transform:'+(translateX)+';'}}"><view class="cl-list-item__container"><block wx:if="{{$slots.icon}}"><view class="cl-list-item__icon"><slot name="icon"></slot></view></block><block wx:if="{{label&&label!='true'}}"><text class="cl-list-item__label">{{label}}</text></block><view class="{{['cl-list-item__content',isJustify,isColor]}}"><slot></slot></view><view class="cl-list-item__append"><slot name="append"></slot></view></view><block wx:if="{{swipe!='none'}}"><view class="{{['cl-list-item__menu-'+swipe]}}"><slot name="menu"></slot></view></block></view></view>